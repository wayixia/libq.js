{
    "applicationName": "Q.js",
    "opts" : {
      "encoding" : "utf8",
      "destination" : "./docs"
    },
    "tags": {
        "allowUnknownTags": true,
        "dictionaries": ["jsdoc","closure"]
    },
    "source": {
        "include": ["src"],
        "includePattern": ".+\\.js(doc)?$",
        "excludePattern": "(^|\\/|\\\\)_"
    },
    "plugins": ["plugins/markdown"],
    "templates": {
        "duoshuo": "true",
       	"googleAnalytics": "UA-63273490-1",
        "cleverLinks": false,
        "monospaceLinks": false,
        "applicationName" : "libqjs"
    }
}
